; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SuperSeaStarSimulatorV1"
run/main_scene="res://scenes/other/menu.tscn"
config/features=PackedStringArray("4.3", "Mobile")
run/max_fps=120
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://art/UI_and_the_like/this_is_a_loading_screen_now.png"
config/icon="res://icon.svg"
boot_splash/minimum_display_time=732

[autoload]

SpellDatabase="*res://scripts/spells/spell_database.gd"
fullscreen_thingy="*res://scripts/UI_and_the_like/fullscreen_thingy.gd"
Spawnstates="*res://scripts/map/open_world_stuff/spawnstates.gd"
Playerglob="*res://scripts/map/open_world_stuff/playerglob.gd"
RoomLoad="*res://scripts/map/open_world_stuff/room_load.gd"
SaveSystem="*res://scripts/other/save_system.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
window/stretch/aspect="ignore"
mouse_cursor/custom_image="res://art/UI_and_the_like/tempcursortest.png"
mouse_cursor/custom_image_hotspot=Vector2(16, 16)

[file_customization]

folder_colors={
"res://art/particle textures/": "blue",
"res://art/particles/": "teal",
"res://scenes/levels/open_world_mode/desert1/": "yellow",
"res://scenes/levels/open_world_mode/forest1start/": "green",
"res://scripts/map/open_world_stuff/": "teal",
"res://scripts/particles/": "teal"
}

[global_group]

player="this is a player"
saveables=""
bridge=""

